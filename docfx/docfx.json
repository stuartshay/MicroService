{
  "metadata": [
    {
      "src": [
        {
          "cwd": "../",
          "files": [
            "src/MicroService.Common/MicroService.Common.csproj",
            "src/MicroService.Data/MicroService.Data.csproj",
            "src/MicroService.Service/MicroService.Service.csproj",
            "src/MicroService.WebApi/MicroService.WebApi.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**.yaml",
          "api/**.json",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "restapi/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "articles/images/**.png",
          "restapi/**.html",
          "restapi/**.js",
          "restapi/**.json"
        ],
        "exclude": [
          "restapi/toc.yml"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}